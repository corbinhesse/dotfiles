{
	"coc.preferences.bracketEnterImprove": true,
	"coc.preferences.formatOnSaveFiletypes": [],
	"coc.preferences.jumpCommand": "drop",

	"diagnostic.errorSign": "",
	"diagnostic.hintSign": "",
	"diagnostic.infoSign": "",
	"diagnostic.warningSign": "",

	"explorer.file.child.template": " [indent][selection][filename omitCenter 1][readonly]",
	"explorer.file.column.indent.chars": " ",
	"explorer.file.column.indent.indentLine": true,
	"explorer.file.hiddenRules": {
		"extensions": [],
		"filenames": [],
		"patternMatches": ["^\\.git$", "^\\.DS_Store", "^node_modules", "^\\.cache"]
	},
	"explorer.file.root.template": "[root]",
	"explorer.filename.colored.enable": false,
	"explorer.git.enable": false,
	"explorer.icon.selected": "* ",
	"explorer.openAction.strategy": "select",
	"explorer.previewAction.onHover": false,
	"explorer.icon.enableNerdfont": false,
	"explorer.icon.customIcons": {
		"icons": {
			"file": {
				"code": " ",
				"color": "#ffffff"
			}
		},
		"extensions": {},
		"filenames": {},
		"dirnames": {},
		"patternMatches": {},
		"dirPatternMatches": {}
	},
	"explorer.sources": [
		{
			"name": "file",
			"expand": true
		}
	],
	"explorer.width": 30,

	"git.addGBlameToVirtualText": true,
	"git.addedSign.hlGroup": "Green",
	"git.addedSign.text": "▎",
	"git.changeRemovedSign.hlGroup": "Orange",
	"git.changeRemovedSign.text": "▎",
	"git.changedSign.hlGroup": "Yellow",
	"git.changedSign.text": "▎",
	"git.command": "git",
	"git.enableGutters": true,
	"git.removedSign.hlGroup": "Red",
	"git.removedSign.text": "▁",
	"git.showCommitInFloating": false,
	"git.topRemovedSign.hlGroup": "Red",
	"git.topRemovedSign.text": "▔",
	"git.virtualTextPrefix": " ",

	"highlight.colorNames.enable": false,

	"http.proxyStrictSSL": false,

	"list.indicator": "▸",
	"list.insertMappings": {
		"<C-c>": "do:toggle",
		"<C-t>": "action:tabe",
		"<C-s>": "action:split",
		"<C-v>": "action:vsplit",
		"<C-w>": "action:preview",
		"<C-x>": "do:switch",
		"<C-l>": "do:exit"
	},
	"list.nextKeymap": "<C-n>",
	"list.previousKeymap": "<C-p>",
	"list.source.files.args": ["--hidden", "--files", "--no-ignore"],
	"list.menuAction": false,
	"list.statusLineSegments": [
		"%#CocListMode#-- %{coc#list#status(\"mode\")} --%*",
		"%{coc#list#status(\"loading\")}",
		"%{coc#list#status(\"args\")}",
		"(%L/%{coc#list#status(\"total\")})",
		"%=",
		"%#CocListPath# %{coc#list#status(\"cwd\")} %l/%L%*"
	],

	"javascript.format.insertSpaceAfterCommaDelimiter": true,
	"javascript.format.insertSpaceAfterConstructor": true,
	"javascript.preferences.importModuleSpecifier": "relative",
	"javascript.preferences.quoteStyle": "single",
	"javascript.suggest.completeFunctionCalls": true,
	"javascript.suggestionActions.enabled": true,

	"notification.highlightGroup": "Gray600",

	"prettier.arrowParens": "always",
	"prettier.formatterPriority": 1,
	"prettier.semi": false,
	"prettier.singleQuote": true,

	"signature.hideOnTextChange": true,
	"signature.preferShownAbove": false,

	"snippets.userSnippetsDirectory": "~/.vim/snippets",
	"snippets.extends": {
		"javascriptreact": ["javascript"],
		"typescriptreact": ["javascript"],
		"typescript": ["javascript"]
	},

	"sql.formatOptions": {
		"linesBetweenQueries": 1
	},
	"sql.lintOnChange": true,
	"sql.lintOnSave": true,

	"suggest.enablePreselect": true,
	"suggest.maxCompleteItemCount": 50,
	"suggest.virtualText": false,
	"suggest.floatConfig": {
		"border": false,
		"borderhighlight": "CocFloatingBorder",
		"highlight": "CocFloating"
	},
	"suggest.noselect": false,

	"tsserver.formatOnType": false,

	"yank.enableCompletion": false,
	"yank.highlight.duration": 250
}
